mixin header(prev,next)
	.header__container.container(class=attributes.class) 
		header.header
			a.logo.href(href="/" data-link) 
				|Mythril 
				br
				|Studio 
			nav.nav 
				ul.nav__list 
					li.nav__item 
						a.nav__link.href(href="/services" data-link) Services
					li.nav__item 
						a.nav__link.href(href="/nft" data-link) NFT
					li.nav__item 
						a.nav__link.href(href="/jobs" data-link) Jobs
					li.nav__item 
						a.nav__link.href(href="/about" data-link) About
					li.nav__item 
						a.nav__link.href(href="/contacts" data-link) Contacts
			.nav-mobile
				a.nav-mobile__item(href=prev data-link)
					|prev
				a.nav-mobile__item.nav-mobile__item--reversed(href=next data-link)
					|next
				button.nav-mobile__button
					span
		.mobile-nav-wrapper 
			ul.network 
				li.network__item
					a.network__link(href="") 
						i.bx.bxl-behance
				li.network__item
					a.network__link(href="")
						i.bx.bxl-facebook
				li.network__item
					a.network__link(href="")
						i.bx.bxl-twitter
				li.network__item
					a.network__link(href="")
						i.bx.bxl-instagram
				li.network__item
					a.network__link(href="")
						i.bx.bxl-linkedin
			ul.nav__list.nav-mobile__list 
				li.nav__item 
					a.nav__link(href="/services" data-link) Services
				li.nav__item 
					a.nav__link(href="/nft" data-link) NFT
				li.nav__item 
					a.nav__link(href="/jobs" data-link) Jobs
				li.nav__item 
					a.nav__link(href="/about" data-link) About
				li.nav__item 
					a.nav__link(href="/contacts" data-link) Contacts
			a(href="#").header__mail 				info@mythril.space
			
			